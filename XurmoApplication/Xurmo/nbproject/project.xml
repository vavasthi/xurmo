<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.kjava.j2meproject</type>
    <configuration>
        <configurations xmlns="http://www.netbeans.org/ns/project-configurations/1">
            <configuration>GENERIC</configuration>
            <configuration>MOTORAZR_maxx_V6</configuration>
        </configurations>
        <data xmlns="http://www.netbeans.org/ns/j2me-project">
            <name>Xurmo</name>
            <minimum-ant-version>1.6</minimum-ant-version>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/2">
            <reference>
                <foreign-project>Fire</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.Fire}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
                <properties>
                    <property name="config.active"/>
                </properties>
            </reference>
            <reference>
                <foreign-project>J2MELib</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.J2MELib}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar.GENERIC</id>
                <properties>
                    <property name="config.active">GENERIC</property>
                </properties>
            </reference>
            <reference>
                <foreign-project>MicroEWT</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.MicroEWT}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar.GENERIC</id>
                <properties>
                    <property name="config.active">GENERIC</property>
                </properties>
            </reference>
        </references>
    </configuration>
</project>

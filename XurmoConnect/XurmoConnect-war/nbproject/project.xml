<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>XurmoConnect-war</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <explicit-platform explicit-source-supported="true"/>
            <web-module-libraries>
                <library files="1">
                    <file>${reference.XurmoConnect-ejb.dist}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <web-service-clients>
                <web-service-client>
                    <web-service-client-name>RegisterUserBean</web-service-client-name>
                    <web-service-stub-type>jsr-109_client</web-service-stub-type>
                    <client-source-url>http://localhost:8080/XurmoConnect-ejb/RegisterUserBean?wsdl</client-source-url>
                </web-service-client>
            </web-service-clients>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>XurmoConnect-ejb</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>dist</target>
                <clean-target>clean</clean-target>
                <id>dist</id>
            </reference>
        </references>
    </configuration>
</project>
